1.1.3.1
SELECT nombre FROM producto
1.1.3.2
SELECT nombre, precio FROM producto
1.1.3.3
SELECT * FROM producto
1.1.3.4
select nombre, precio , (precio/0.9) from producto
1.1.3.5
nomobre as nombre, precio as euros, (precio/0.9) as dolares from producto
1.1.3.6
select (UPPER(nombre)) as nombres, precio as precio from producto
1.1.3.7
select (Lower(nombre)) as nombres, precio as precio from producto
1.1.3.8
SELECT nombre, SUBSTRING(upper(nombre), 1, 2) FROM fabricante
1.1.3.9
SELECT nombre, round (precio,1) FROM producto
1.1.3.10
SELECT nombre, TRUNCATE(precio,0) FROM producto
1.1.3.11
SELECT codigo_fabricante FROM producto
1.1.3.12
SELECT distinct codigo_fabricante FROM producto
1.1.3.13
SELECT nombre FROM fabricante order by nombre ASC
1.1.3.14
SELECT nombre FROM fabricante order by nombre DESC
1.1.3.15
SELECT nombre FROM producto order by nombre ASC
SELECT nombre FROM producto order by precio DESC
1.1.3.16
SELECT * FROM fabricante LIMIT 0, 5
1.1.3.17
SELECT * FROM fabricante LIMIT 3, 2
1.1.3.18
SELECT * FROM producto order by nombre ASC LIMIT 0, 1
1.1.3.19
SELECT * FROM producto order by nombre DESC LIMIT 0, 1
1.1.3.20
SELECT nombre FROM producto where codigo_fabricante=2
1.1.3.21
SELECT nombre FROM producto where precio<=120
1.1.3.22
SELECT nombre FROM producto where precio>=400
1.1.3.23
SELECT nombre FROM producto where precio<400
1.1.3.24
SELECT nombre,precio FROM producto where precio>80 and precio<300
1.1.3.25
SELECT nombre,precio FROM producto where precio BETWEEN 60 and 200
